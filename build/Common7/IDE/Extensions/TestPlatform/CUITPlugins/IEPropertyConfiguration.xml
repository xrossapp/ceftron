<?xml version="1.0" encoding="utf-8"?>
<UITestSearchPropertyConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:noNamespaceSchemaLocation="IEPropertyConfiguration.xsd">
 <!-- Default Property Configuration for IE plugin -->
 <!-- ApplySmartConfiguration feature to apply a smart logic over this configuration -->
 <!-- ApplySmartConfiguration might cause some properties to move between
      search and filter properties based on their values or even get removed as well.
     (for example, due to invalid values on some controls for those properties. -->
 
 <ControlTypes ApplySmartConfiguration="true">
  
   <ControlType Name="Audio">
      <SearchProperties>
           <Property Name="Id"/>
           <Property Name="Name"/>
           <Property Name="Title"/>
      </SearchProperties>
      <FilterProperties>
           <Property Name="Src"/>
           <Property Name="Class"/>
           <Property Name="ControlDefinition"/>
           <Property Name="TagInstance"/>
      </FilterProperties>
   </ControlType>
   
   <ControlType Name="Body">
      <SearchProperties>
           <Property Name="Id"/>
           <Property Name="RedirectingPage"/>
           <Property Name="FrameDocument"/>
      </SearchProperties>
      <FilterProperties>
           <Property Name="Title"/>
           <Property Name="AbsolutePath"/>
           <Property Name="PageUrl"/>
      </FilterProperties>
   </ControlType>

   <ControlType Name="Button">
      <SearchProperties>
           <Property Name="Id"/>
           <Property Name="Name"/>
           <Property Name="DisplayText"/>
           <Property Name="Type"/>
      </SearchProperties>
      <FilterProperties>
           <Property Name="Src"/>
           <Property Name="Title"/>
           <Property Name="Class"/>
           <Property Name="ControlDefinition"/>
           <Property Name="TagInstance"/>
      </FilterProperties>
   </ControlType>

   <ControlType Name="CheckBox">
      <SearchProperties>
           <Property Name="Id"/>
           <Property Name="Name"/>
           <Property Name="Value"/>
           <Property Name="LabeledBy"/>
      </SearchProperties>
      <FilterProperties>
           <Property Name="Title"/>
           <Property Name="Class"/>
           <Property Name="ControlDefinition"/>
           <Property Name="TagInstance"/>
      </FilterProperties>
   </ControlType>

   <ControlType Name="ComboBox">
      <SearchProperties>
           <Property Name="Id"/>
